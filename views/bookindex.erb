<a href="/index">Home</a>
<h1>Listing Books<h1/>

<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Author First Name</th>
    <th>Author Last Name</th>
    <th>Checked Out>
  </tr>

<% @books.each do |book| %>
  <tr>
    <td><%= book.id %></td>
    <td><%= book.title %></td>
    <td><%= book.author_first_name %></td>
    <td><%= book.author_last_name %></td>
    <td><%= book.checked_out_flag %></td>
  </tr>
<% end %>
</table>

